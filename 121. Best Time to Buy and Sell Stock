class Solution {
    public int maxProfit(int[] prices) {
        int len = prices.length;
        if(len == 0 || len == 1) return 0;
        int buy = prices[0];
        int sell = 0;
        for(int i = 1;i < len;i++) {
            if(prices[i] > buy) {
                sell = Math.max(sell,prices[i] - buy);
            }else {
                buy = prices[i];
            }
        }
        return sell;
    }
}
